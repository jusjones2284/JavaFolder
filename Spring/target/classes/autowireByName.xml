<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="specification" class="com.aw.example.autowire.name.Specification">
        <property name="make" value="Toyota"></property>
<!--        <property name="make" value="Honda"></property>-->


    </bean>
    <bean id="myCar" class="com.aw.example.autowire.name.Car" autowire="byName"/>



</beans>